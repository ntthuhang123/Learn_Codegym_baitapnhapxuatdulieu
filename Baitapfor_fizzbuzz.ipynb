   "source": [
    "a = input(\"Nhap vao hai so: \")\n",
    "chuoi = a.split()\n",
    "soDau = int(chuoi[0])\n",
    "soCuoi = int(chuoi[1])\n",
    "for i in range(soDau,soCuoi+1):\n",
    "    if i % 15 == 0:\n",
    "        print(\"FizzBuzz\")\n",
    "    elif i % 3 == 0:\n",
    "        print(\"Fizz\")\n",
    "    elif i % 5 == 0:\n",
    "        print(\"Buzz\")\n",
    "    else:\n",
    "        print(i)"

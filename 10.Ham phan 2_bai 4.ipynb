   "source": [
    "def find_min_inlist(mylist):\n",
    "    min_number = mylist[0]\n",
    "    for i in mylist:\n",
    "        if i < min_number:\n",
    "            min_number = i\n",
    "    return  min_number\n",
    "print(find_min_inlist([9,5,100,3]))"
   ]
